    "@types/jest": "^29.5.14",
    "@types/testing-library__jest-dom": "^5.14.9",
    "eslint": "^8.48.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "prettier": "^3.0.3",
    "turbo": "latest"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "packageManager": "npm@9.8.1"
